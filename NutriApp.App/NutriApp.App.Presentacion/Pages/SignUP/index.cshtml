        <br>
        <img src="~/assets/NutriApp.svg" alt="NutriApp title" style="width: 30%;">


        <form class="formLogin" method="post">
            <div class="formContent">
                <label for="nameUser">Nombre:</label>
                <input type="text" name="nameUser" class="signup" required>
                <label for="dateUser">Fecha de nacimiento:</label>
                <input type="date" name="dateUser" class="signup" required>
                <label for="telefonoUser">Telefono:</label>
                <input type="text" name="telefonoUser" class="signup">
                <label for="emailUser">Correo:</label>
                <input type="email" name="emailUser" class="signup" required>
                <label for="passwordUser">Contraseña:</label>
                <input type="password" name="passwordUser" class="signup" required>
                <span>Seleccione el tipo de cuenta</span>
                <hr>
                <div class="userType">
                    
                    <input type="radio" name="userType" id="pacienteType" value="paciente">
                    <label for="pacienteType">
                        <img src="~/assets/paciente.png" alt="paciente icon">
                        Paciente
                    </label>

                    <input type="radio" name="userType" id="nutriType" value="nutricionista">
                    <label for="nutriType">
                        <img src="~/assets/nutricionista.png" alt="nutricionista icon">
                        Nutricionista
                    </label>

                    <input type="radio" name="userType" id="coachType" value="coach">
                    <label for="coachType">
                        <img src="~/assets/coach.png" alt="coach icon">
                        Coach
                    </label>
                </div>

                <div class="pacienteSign">
                    <input type="button" class="close" value="X" id="paciente"></button>
                    <br>
                    <label for="Direccion">Direccion: (Opcional)</label>
                    <input type="text" name="Direccion" class="signup" asp-for="Paciente.Direccion">
                    <label for="Longitud">Longitud:</label>
                    <input type="text" name="Longitud" class="signup"  asp-for="Paciente.Longitud">

                    <label for="Latitud">Latitud:</label>
                    <input type="text" name="Latitud" class="signup"  asp-for="Paciente.Latitud"> 
                </div>
                <div class="nutricionistaSign">
                    <input type="button" class="close" value="X" id="nutricionista"></button>
                    <br>
                    <label for="Especializacion">Especialización:</label>
                    <input type="text" name="Especializacion" class="signup" asp-for="Nutricionista.Especializacion">

                    <label for="TarjetProfesional">N° tarjeta profesional:</label>
                    <input type="text" name="TarjetaProfesional" class="signup" asp-for="Nutricionista.TarjetaProfesional"> 
                </div>
                <div class="coachSign">
                    <input type="button" class="close" value="X" id="coach"></button>
                    <br>
                    <label for="Especializacion">Especialización:</label>
                    <input type="text" name="Especializacion" class="signup" asp-for="Coach.Especializacion">

                    <label for="TarjetProfesional">N° tarjeta profesional:</label>
                    <input type="text" name="TarjetaProfesional" class="signup" asp-for="Coach.TarjetaProfesional">
                </div>
            </div>
            <button type="submit" class="btn-rdn">Siguiente</button>
            <br>
            <a asp-page="/Login/index">¿Ya tienes una cuenta?</a>
        </form>
    </main>

    <script src="~/js/site.js" asp-append-version="true"></script>
</body>

</html>